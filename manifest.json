{
    "manifest_version": 2,
    "name": "ESM Chrome Extension",
    "description": "A Chrome Extension for GWN ESM",
    "version": "0.5",
    "options_page":"popup.html",
    "update_url": "http://shewlayce.com/dev/T2Labs/updates.xml",
    "background": {
        "page": "background.html"
    },
    "permissions": [
        "tabs",
        "notifications",
        "storage",
        "http://services.hq/"
    ],
    "browser_action": {
        "default_title": "ESM Chrome Extension",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
