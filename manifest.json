{
    "manifest_version": 2,
    "name": "GWN RealTime",
    "description": "A Jira notification and auto-updater.",
    "version": "1.1",
    "icons": {  "16": "images/icon.png",
                "48": "images/icon.png",
               "128": "images/icon.png"
    },
    "web_accessible_resources": [
        "images/icon.png"
    ],
    "options_page":"popup.html",
    "update_url": "http://shewlayce.com/dev/T2Labs/updates.xml",
    "background": {
        "page": "background.html"
    },
    "permissions": [
        "tabs",
        "notifications",
        "storage",
        "http://services.hq/"
    ],
    "browser_action": {
        "default_title": "GWN RealTime",
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
